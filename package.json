{
  "name": "infinitedoc",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run dev:studio",
    "dev:studio": "pnpm --filter @infinitedoc/studio dev",
    "dev:publish": "pnpm --filter @infinitedoc/publish dev",
    "dev:all": "pnpm -r --parallel --filter @infinitedoc/studio --filter @infinitedoc/publish dev",
    "build": "pnpm -r --filter @infinitedoc/studio --filter @infinitedoc/publish build",
    "build:studio": "pnpm --filter @infinitedoc/studio build",
    "build:publish": "pnpm --filter @infinitedoc/publish build",
    "lint": "pnpm -r --filter @infinitedoc/studio --filter @infinitedoc/publish lint",
    "lint:studio": "pnpm --filter @infinitedoc/studio lint",
    "lint:publish": "pnpm --filter @infinitedoc/publish lint",
    "preview:studio": "pnpm --filter @infinitedoc/studio preview",
    "preview:publish": "pnpm --filter @infinitedoc/publish preview"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
